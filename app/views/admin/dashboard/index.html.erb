    <%= link_to "Create Another Admin", new_admin_dashboard_path %>
    </br>
    <%= link_to "Delete My Account", user_path(current_user.id), method: :delete, data: {confirm: 'Are you sure?'} %>
  </div>
</div>
</br>
</br>
<%= button_to "Manage Creatures", admin_recipients_path, method: :get, class: "btn-admin" %>
